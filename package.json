{
  "name": "gpx-viewer",
  "version": "0.1.1",
  "description": "GPX Viewer.",
  "main": "src/main.js",
  "scripts": {
    "clean": "yarn cache clean && del-cli dist deps node_modules \"test/coverage\" bin",
    "clean:dist": "del-cli dist",
    "preclean:install": "yarn run clean",
    "clean:install": "yarn install",
    "preclean:start": "yarn run clean",
    "clean:start": "yarn start",
    "watch": "yarn run watch:dev",
    "watch:dev": "yarn run build:dev -- --watch",
    "watch:dev:hmr": "yarn run watch:dev -- --hot",
    "watch:prod": "yarn run build:prod -- --watch",
    "build": "yarn run prepare ; yarn run build:dev",
    "prebuild:dev": "yarn run clean:dist; yarn run prepare",
    "build:dev": "cross-env NODE_ENV=development yarn run webpack -- --progress --profile",
    "prebuild:prod": "yarn run clean:dist; yarn run prepare",
    "build:prod": "cross-env NODE_ENV=production yarn run webpack -- --progress --profile",
    "start": "yarn run prepare ; yarn run server:dev",
    "server": "yarn run server:dev",
    "server:dev": "cross-env NODE_ENV=development yarn run webpack-dev-server -- --inline --progress --profile --watch",
    "server:dev:hmr": "yarn run server:dev -- --hot",
    "server:prod": "http-server bin --cors",
    "webpack": "cross-env BABEL_ENV=node ./node_modules/.bin/webpack",
    "webpack-dev-server": "cross-env BABEL_ENV=node ./node_modules/.bin/webpack-dev-server",
    "prepare": "yarn run clean:dist ; ./retreive-github-deps.sh",
    "lint": "eslint src/**",
    "update-deps": "yarn-update"
  },
  "keywords": [
    "map",
    "viewer",
    "gpx"
  ],
  "license": "UNLICENTED",
  "author": "Etienne Vrignaud",
  "aurelia": {
    "build": {
      "resources": []
    }
  },
  "dependencies": {
    "aurelia-binding": "1.1.1",
    "aurelia-bootstrapper-webpack": "1.1.0",
    "aurelia-dependency-injection": "1.3.0",
    "aurelia-event-aggregator": "1.0.1",
    "aurelia-fetch-client": "1.1.0",
    "aurelia-framework": "1.0.8",
    "aurelia-history": "1.0.0",
    "aurelia-history-browser": "1.0.0",
    "aurelia-loader": "1.0.0",
    "aurelia-loader-webpack": "2.0.0",
    "aurelia-logging": "1.2.0",
    "aurelia-logging-console": "1.0.0",
    "aurelia-metadata": "1.0.3",
    "aurelia-pal": "1.2.0",
    "aurelia-pal-browser": "1.1.0",
    "aurelia-path": "1.1.1",
    "aurelia-polyfills": "1.1.1",
    "aurelia-route-recognizer": "1.1.0",
    "aurelia-router": "1.1.1",
    "aurelia-task-queue": "1.1.0",
    "aurelia-templating": "1.2.0",
    "aurelia-templating-binding": "1.2.0",
    "aurelia-templating-resources": "1.2.0",
    "aurelia-templating-router": "1.0.1",
    "babel-polyfill": "6.22.0",
    "bluebird": "3.4.7",
    "d3": "3.5.17",
    "jquery": "3.1.1",
    "leaflet": "1.0.3",
    "leaflet-graphicscale": "0.0.2",
    "leaflet-providers": "1.1.16",
    "leaflet.layerscontrol-minimap": "1.0.11",
    "normalize.css": "5.0.0",
    "randomcolor": "0.4.4",
    "regenerator-runtime": "0.10.1"
  },
  "devDependencies": {
    "@easy-webpack/config-aurelia": "2.2.3",
    "@easy-webpack/config-babel": "4.0.2",
    "@easy-webpack/config-common-chunks-simple": "2.0.3",
    "@easy-webpack/config-copy-files": "1.1.2",
    "@easy-webpack/config-css": "4.0.1",
    "@easy-webpack/config-env-development": "2.1.6",
    "@easy-webpack/config-env-production": "3.0.0",
    "@easy-webpack/config-external-source-maps": "3.1.0",
    "@easy-webpack/config-fonts-and-images": "3.0.0",
    "@easy-webpack/config-generate-index-html": "2.1.1",
    "@easy-webpack/config-global-bluebird": "2.1.0",
    "@easy-webpack/config-global-jquery": "2.1.1",
    "@easy-webpack/config-global-regenerator": "1.3.0",
    "@easy-webpack/config-html": "3.1.0",
    "@easy-webpack/config-json": "3.1.0",
    "@easy-webpack/config-test-coverage-istanbul": "3.2.0",
    "@easy-webpack/config-uglify": "2.2.3",
    "@easy-webpack/core": "2.0.1",
    "aurelia-protractor-plugin": "1.0.1",
    "aurelia-tools": "1.0.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-env": "1.1.8",
    "babel-register": "6.22.0",
    "concurrently": "3.1.0",
    "cross-env": "3.1.4",
    "del-cli": "0.2.1",
    "eslint": "3.14.1",
    "http-server": "0.9.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.12"
  }
}
